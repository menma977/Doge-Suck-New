<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dogesuck">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".content.bot.CustomActivity"></activity>
        <activity android:name=".content.bot.FlyFibonacciActivity" />
        <activity android:name=".content.bot.FibonacciActivity" />
        <activity android:name=".content.bot.ReverseLabouchereActivity" />
        <activity android:name=".content.bot.LabouchereActivity" />
        <activity
            android:name=".content.bot.ClassicActivity"
            android:label="@string/classic"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".content.bot.ManualBotActivity"
            android:label="@string/manual"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".content.HomeActivity" />
        </activity>
        <activity
            android:name=".content.HomeActivity"
            android:theme="@style/AppThemeNoActionBar" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/AppThemeNoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppThemeNoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>